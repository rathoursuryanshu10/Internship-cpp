#include <iostream>
using namespace std;

int main() {
	int n,i,a,b,c;
	float sum,avg;
	cin>>n;
	for(i=0;i<n;i++)
	{
	    cin>>a;
	    cin>>b;
	    cin>>c;
	    sum=a+b;
	    avg=sum/2;
	    if(avg>c)
	    {
	        cout<<"YES" <<"\n";
	    }
	    else
	    {
	        cout<<"NO" <<"\n";
	    }
	}
	return 0;
}
